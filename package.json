{
  "name": "svelte5-vite-ts-tailwind-eslint",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check vite.config.ts src && eslint vite.config.ts src && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write vite.config.ts src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "archive": "npm run build && (cd dist && zip -r ../svelte5-vite-ts-tailwind-eslint.zip * && tar -czf ../svelte5-vite-ts-tailwind-eslint.tar.gz *)"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@tsconfig/svelte": "^5.0.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^3.5.1",
    "flowbite-svelte": "^1.0.0-next.18",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "globals": "^15.15.0"
  }
}
